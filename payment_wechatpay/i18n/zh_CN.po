#. module: payment_wechatpay
#: model_terms:payment.acquirer,cancel_msg:payment_wechatpay.payment_acquirer_wechatpay
msgid "<span><i>Cancel,</i> Your payment has been cancelled.</span>"
msgstr "<span><i>取消,</i> 您的付款已取消.</span>"

#. module: payment_wechatpay
#: model_terms:payment.acquirer,done_msg:payment_wechatpay.payment_acquirer_wechatpay
msgid "<span><i>Done,</i> Your online payment has been successfully processed. Thank you for your order.</span>"
msgstr "<span><i>完成,</i> 您的在线支付已成功完成. 谢谢.</span>"

#. module: payment_wechatpay
#: model_terms:payment.acquirer,error_msg:payment_wechatpay.payment_acquirer_wechatpay
msgid "<span><i>Error,</i> Please be aware that an error occurred during the transaction. The order has been confirmed but will not be paid. Do not hesitate to contact us if you have any questions on the status of your order.</span>"
msgstr "<span><i>错误，</i>请注意，发生错误。订单已经确认，但不会支付。如果您对订单状态有任何疑问，请随时与我们联系.</span>"

#. module: payment_wechatpay
#: model_terms:payment.acquirer,pending_msg:payment_wechatpay.payment_acquirer_wechatpay
msgid "<span><i>Pending,</i> Your online payment has been successfully processed. But your order is not validated yet.</span>"
msgstr "<span><i>待定,</i> 在线支付已完成. 订单未确认.</span>"

